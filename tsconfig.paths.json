{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@router": ["router"],
      "@models": ["models"],
      "@models/*": ["models/*"],
      "@api": ["api"],
      "@api/*": ["api/*"],
      "@assets": ["assets"],
      "@assets/*": ["assets/*"]
    }
  }
}
